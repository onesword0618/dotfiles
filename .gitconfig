[core]
	editor = vim -c \"set fenc=utf=8\"
	ignoreCase = true
	fsmonitor = true
	trustctime = true
	quotePath = false
	autocrlf = false
	symlinks = true
[web "browser"]
	cmd = google-chrome
[color]
	ui = auto
[commit]
	gpgsign = true
	template = ~/.git_commit_template.txt
[help]
	format = web
[alias]
	h = help -a
	ls = ls-files
	st = status
	sb = status -sb
	test = add --dry-run .
	b = branch
	ba = branch -a
	co = checkout
	f = fetch
	fp = fetch --prune
	df = diff
