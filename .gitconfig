[advice]
    fetchShowForcedUpdates = true
    # git-fetchの強制更新で時間がかかっている場合の警告
    pushUpdateRejected = true
    # git-pushの更新拒否が発生した場合の警告
    pushNonFFCurrent = true
    # git-pushに失敗した場合の警告
    pushNonFFMatching = true
    # git-pushを実行時に現在のブランチでないものを指定した場合の警告
    pushAlreadyExists = true
    # git-pushでfast-forwardingできないtagの更新拒否が発生した場合の警告
    pushFetchFirst = true
    # git-pushで更新拒否が発生した場合の警告
    pushNeedsForce = true
    # git-pushでコミットしていないオブジェクトを参照し更新拒否した場合の警告
    pushUnqualifiedRefname = true
    # git-pushで名前空間の推測をやめた場合の警告
    statusAheadBehind = true
    # git-statusの追跡計算で時間がかかっていた場合の警告
    statusHints = true
    # git-status,git-commitで進行指示を提示する
    statusUoption = true
    # git-status実行時に、未追跡のファイルを表示するのに2秒以上かかった場合に-uをつけることを進めることを提示する
    commitBeforeMerge = true
    # git-merge実行時に、マージが拒否された場合にローカルの変更を上書きをしないことを提示する
    resetQuiet = true
    # git-resetに--quietを使用することを提示する
    resolveConflict = true
    # コンフリクトが発生して操作ができない場合のコマンドを提示する
    sequencer = true
    # シーケンスが発生していることを表示する
    implicitIdentity = true
    # 自分の情報が推測された場合の設定方法を提示する
    detachedHead = true
    # git-switch,git-checkoutを使用してdetach HEADになった場合のローカルブランチの提示をする
    checkoutAmbiguousRemoteBranchName = true
    # git-switch,git-checkoutの引数があいまいだった場合の行動を提示する
    amWorkDir = true
    # git-am失敗時にパッチファイルの場所を提示する
    rmHints = true
    # git-rmに失敗した場合の現在の状態から進める指示を提示する
    addEmbeddedRepo = true
    # 他のリポジトリに追加した場合の対処を提示する
    ignoredHook = true
    # hookの設定が無視された場合の対応を提示する
    waitingForEditor = true
    # gitがユーザからのエディタ入力を待っている時にターミナルに出力する
    nestedTag = true
    # タグにタグを重ねた場合に提示する
    submoduleAlternateErrorStrategyDie = true
    # submodule.alternateErrorStrategyにdieに設定してエラーが発生した場合に表示する
    addIgnoredFile = true
    # ignoreに設定しているファイルを追加した場合に表示する
    addEmptyPathspec = true
    # addにパスを設定していない場合に表示する
[core]
    fileMode = true
    # 作業ツリー内のファイルの実行可能ビットを優先するかどうか
    hideDotFiles = true
    # dotで始まるファイルを非表示にするかどうか /Windows only
    ignoreCase = true
    # 大文字小文字を区別しない
    precomposeUnicode = true
    # ファイル名のunicode分解を元に戻す /Mac OS only
    protectHFS = false
    # HFS+ .gitと同等とみなされるチェックアウトを許容しない /true Mac OS | false Linux or Windows
    protectNTFS = false
    # NTFS ファイルシステムで問題になるパスのチェックアウトを許容しない /true Windows | false Linux or Mac OS
    fsmonitor = true
    # 版によって要求された日時以降に変更された可能性のあるすべてのファイルを識別するコマンドとして使用するかどうか
    fsmonitorHookVersion = 2
    # フックのバージョンを指定する
    trustctime = true
    # indexと作業ツリーの間のctimeの違いを無視するかどうか
    splitIndex = false
    # indexのsplit-indexを使用するかどうか
    untrackedCache = true
    # indexのトラックされていないキャッシュを保存するかどうか
    # checkStat
    # 設定値不明
    quotePath = false
    # 制御文字の利用をパス内で異常として検知するかどうか
    eol = native
    # テキストの行末の種類を検出する /autocrlfがtrueの場合は無視される
    safecrlf = warn
    # 行末変換が可能な場合にCRLFに変換することができるかどうか
    autocrlf = false
    # text属性をautoにし、eolをcrlfに設定するのと同じにするかどうか
    checkRoundtripEncoding = SHIFT-JIS
    # カンマまたは空白のエンコーディング
    symlinks = true
    # falseだとリンクテキストを含んだプレーンテキストとしてチェックアウトする
    # gitProxy
    # プロキシコマンドの設定
    # sshCommand
    # ssh接続の代わりにコマンドを実行する
    ignoreStat = false
    # gitの外で変更したファイルを検出するかどうか
    preferSymlinkRefs = false
    # HEADがシンボリックリンクであることｗ期待している古いスクリプトを起動させるときに利用する
    # alternateRefsCommand
    # 未調査
    # alternateRefsPrefixes
    # 未調査
    bare = false
    # trueの場合は、作業ディレクトリが存在しないとみなされる
    # worktree
    # 作業ツリーのルートパスを設定する
    logAllRefUpdates = true
    # reflogを有効にするかどうか
    # repositoryFormatVersion
    # リポジトリのフォーマットとバージョンを設定する
    sharedRepository = false
    # リポジトリの共有範囲を設定する
    warnAmbiguousRefs = true
    # refの曖昧検索を有効にする
    compression = 3
    # 圧縮レベルの設定を行う
    looseCompression = 1
    # パックファイルにないオブジェクトの圧縮レベルを設定する
    # packedGitWindowSize = 1g
    # マッピング操作でパックファイルをメモリに乗せるバイト数
    # packedGitLimit
    # マッピング操作で確保する仮想アドレス空間
    # deltaBaseCacheLimit
    # ベースオブジェクトのキャッシュするために確保するバイト数
    # bigFileThreshold
    # デルタ圧縮を実行するかどうかの判断するサイズ数
    # excludesFile
    # 追跡対象外のパスを記載するパターンを含むファイルパスを指定する
    # askPass
    # 対話的にパスワードを要求するコマンドを利用する場合に設定した値を利用する
    # attributesFile
    # このファイルの属性を調査する. 何をするかは未調査
    # hooksPath
    # $GIT_DIR/hooksディレクトリではなくこちらで指定した場所で探そうとする
    editor = vim -c \"set fenc=utf=8\"
    # メッセージを編集させる場合に利用するエディタを利用する
    # commentChar
    # デフォルトは#
    filesRefLockTimeout=100
    # 個々の参照を再試行する時間をミリ秒単位で指定する
    packedRefsTimeout=100
    # ファイルロックの再試行する時間をミリ秒単位で指定する
    pager=less
    # ページャのコマンドを指定する
    whitespace=space-before-tab,trailing-space
    # 空白とタブの位置が指定した値を満たした場合に、エラーになる
    fsyncObjectFiles=true
    # オブジェクトファイルの書き込み時にfsync()を有効にする
    preloadIndex=true
    # 並列インデックスのプリロードを有効にする
    # createObject
    # 未調査
    notesRef="refs/notes/commits"
    # 未調査
    # sparseCheckout
    # 未調査
    abbrev=auto
    # オブジェクト名が省略される長さを設定する
[am]
    keepcr=true
    # 未調査
    threeWay=false
    # 未調査
[apply]
    ignoreWhitespace=false
    # 空白の変更を検知するようになる
    whitespace=warn
    # 空白に対しての動作を定義する
[blame]
    showRoot=false
    # ルートコミットを境界線として扱わないようする
    blankBoundary=false
    # 境界コミットのコミットオブジェクト名を空白にするかどうか
    showEmail=false
    # 作成者の名前の代わりにメールアドレスを表示するようにするかどうか
    date=iso
    # 日付出力のフォーマットを定義する
[branch]
    autoSetupMerge=true
    # 新しいブランチを作成するときに適切にマージをするようにする
    autoSetupRebase=never
    # 新しいブランチを作成するときに適切にリベースをするようにする
[web]
    browser=chrome
    #cmd="/usr/bin/google-chrome"
[clean]
    requireForce=true
    # f、-i あるいは -n が与えられない限り git-clean を何もしないようにする
[color]
    branch=auto
    diff=always
    colorMoved=true
    ui=auto
    grep=always
    interactive=always
    pager=true
    showBranch=true
    status=always
    # current,local,remote,upstream,plain
[column]
    ui=always
    branch=always
    clean=always
    status=always
    tag=always
[commit]
    status=true
    gpgsign=true
    template = ~/dotfiles/git/template/.git_commit_template.txt
    #cleanup
    #verbose
[credential]
    useHttpPath=false
    #helper
    #username
[credentialCache]
    #ignoreSIGHUP
[pull]
    ff=true
    rebase=false
[push]
    default=current
    gpgSign=true
[rebase]
    stat=false
    autoSquash=false
    autoStash=false
    missingCommitsCheck=warn
    abbreviateCommands=false
[help]
    format = web
[alias]
    b = branch
    ba = branch -a
    br = branch -r
    bm = branch -m
    be = branch --edit-description
    co = checkout
    df = diff
    edit = config -e
    f = fetch
    fp = fetch --prune
    h = help -a
    list = config -l
    ls = ls-files
    new = switch -c
    st = status
    sb = status -sb
    test = add --dry-run .
    trash = rm -r --cached .
[includeIf "gitdir:~/workspace/"]
    # https://git-scm.com/docs/git-config#_conditional_includes
    path = ~/dotfiles/git/config/gitconfig.gitlab
