[user]
    name = onesword0618
    email = ao.akua.leo@gmail.com
    signingkey = E2F30CEB4AEE75BE
[core]
    editor = vim -c \"set fenc=utf=8\"
    ignoreCase = true
    fsmonitor = true
    trustctime = true
    quotePath = false
    autocrlf = false
    symlinks = true
[web "browser"]
    cmd = google-chrome
[color]
    ui = auto
[commit]
    gpgsign = true
    template = ~/dotfiles/git/template/.git_commit_template.txt
[pull]
    rebase = false
[help]
    format = web
[alias]
    b = branch
    ba = branch -a
    br = branch -r
    bm = branch -m
    co = checkout
    df = diff
    edit = config -e
    f = fetch
    fp = fetch --prune
    h = help -a
    list = config -l
    ls = ls-files
    new = switch -c
    st = status
    sb = status -sb
    test = add --dry-run .
    trash = rm -r --cached .
[includeIf "gitdir:~/workspace/"]
    path = ~/dotfiles/git/config/gitconfig.gitlab
